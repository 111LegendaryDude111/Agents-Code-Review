GITHUB_TOKEN=ghp_replace_with_your_token
GITHUB_REPOSITORY=owner/repo
PR_NUMBER=1

# LLM provider: huggingface | gemini | openai | ollama | vllm
LLM_PROVIDER=huggingface
LLM_MODEL=Qwen/Qwen2.5-Coder-32B-Instruct
LLM_BASE_URL=https://router.huggingface.co/v1
HF_TOKEN=replace_with_hf_token

# Hugging Face overrides (optional)
# HUGGINGFACE_API_KEY=hf_...
# HUGGINGFACE_BASE_URL=https://router.huggingface.co/v1
# HUGGINGFACE_MODEL=Qwen/Qwen2.5-Coder-32B-Instruct

# Gemini (optional when LLM_PROVIDER=gemini)
# GEMINI_API_KEY=AIza...

# OpenAI (optional when LLM_PROVIDER=openai)
# OPENAI_API_KEY=sk-...

# Ollama (optional when LLM_PROVIDER=ollama)
# OLLAMA_BASE_URL=http://127.0.0.1:11434/v1
# OLLAMA_MODEL=qwen2.5-coder:7b
# OLLAMA_API_KEY=dummy

# vLLM (optional when LLM_PROVIDER=vllm)
# VLLM_BASE_URL=http://127.0.0.1:8000/v1
# VLLM_MODEL=Qwen/Qwen2.5-Coder-14B-Instruct
# VLLM_API_KEY=dummy
